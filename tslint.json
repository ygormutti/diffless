{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "interface-name": false,
        "callable-types": false,
        "max-classes-per-file": false,
        "quotemark": [
            true,
            "single"
        ],
        "arrow-parens": false,
        "array-type": false,
        "member-ordering": false,
        "member-access": [
            true,
            "no-public"
        ],
        "curly": [
            true,
            "ignore-same-line"
        ]
    }
}